<div class="question">
<h1><%=@question.title%></h1>
<h5><%=@question.user.username%></h5>
<p><%=@question.description%></p>
</div>

<h2><%= @question.answers.count%> Answers</h2>
<div class="answer">
<% @question.answers.each do |answer| %>
<p><%= answer.description %></p>
<h5><%=answer.user.username%></h5>
<% end %>
</div>
